{
    "data": {
        "dataset_name": "ccs",
        "data_path": "data/ccs/preprocessed",
        "dimension_tables": [
            "product"
        ]
    },
    "model": {
        "gnn_aggr": "sum",
        "d_layers": [
            512,
            1024,
            1024,
            1024,
            1024,
            512
        ],
        "dropout": 0.0,
        "dim_t": 128
    },
    "diffusion": {
        "num_timesteps": 2000,
        "gaussian_loss_type": "mse",
        "scheduler": "cosine"
    },
    "trainer": {
        "lr": 6e-4,
        "weight_decay": 1e-5,
        "steps": 200000
    },
    "sampler": {
        "sample_batch_size": 20000,
        "size_multiplier": 1.0
    }
}
